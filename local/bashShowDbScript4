export tableSeparator="\n############################################################################"
export fieldSeparator="----------------------------------------------------------------------------"

echo "tb_user_type" && python3 runSql.py designedDb.db "pragma table_info(tb_user_type)" && echo -e $fieldSeparator && python3 runSql.py designedDb.db "select * from tb_user_type" && echo -e $tableSeparator &&

